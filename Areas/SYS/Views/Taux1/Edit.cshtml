@model RhSensoWeb.Areas.SYS.DTOs.Taux1Dto
@{
    ViewData["Title"] = "Editar Tabela Auxiliar";
    ViewBag.IsEdit = true;
}

<h1 class="mb-3">@ViewData["Title"]</h1>

<form asp-area="SYS" asp-controller="Taux1" asp-action="Edit" method="post">
    @Html.AntiForgeryToken()
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <partial name="_FormFields" model="Model" />

    <div class="mt-4 d-flex gap-2">
        <button type="submit" class="btn btn-primary">Salvar</button>
        <a asp-area="SYS" asp-controller="Taux1" asp-action="Index" class="btn btn-secondary">Voltar</a>
    </div>
</form>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
